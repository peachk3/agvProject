<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <!-- SQL Server 연결 예시 -->
    <add name="DefaultConnection" 
         connectionString="Data Source=localhost;Initial Catalog=AGVProjectDB;Integrated Security=True;TrustServerCertificate=True"
         providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <appSettings>
    <add key="DatabaseType" value="SqlServer" />
    <add key="ConnectionStringName" value="DefaultConnection" />
  </appSettings>
</configuration> 